<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.maxoldshein.dvdlist">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="TV Shows"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".ViewShowActivity" />
        <activity android:name=".AddShowActivity" />
        <activity android:name=".DeleteShowActivity" />
        <activity android:name=".EditShowActivity" />
        <activity android:name=".EditCastActivity"></activity>
    </application>

</manifest>